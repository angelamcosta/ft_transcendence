{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "lib": ["ES2015", "DOM"],
      "outDir": "public",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["ts/**/*.ts"]
}